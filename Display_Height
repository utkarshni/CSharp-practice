using System;

class Program
{
    public static string GetHeightCategory(int heightCm)
    {
        if (heightCm < 150)
            return "Short";
        else if (heightCm < 180)
            return "Average";
        else
            return "Tall";
    }

    static void Main()
    {
        int height = 165;

        Console.WriteLine(GetHeightCategory(height));
    }
}
